# حلقه برای اعداد 0 تا 1000
for n in range(10, 1001):
    if n > 1 : 
        #  آیا عدد بزرگتر از1 است؟
        is_prime = True
        # فرض میکنیم عدد اول است
        for i in range(2, int(n **0.5) + 1):
            # این حلقه برای این است که بررسی کنیم آیا عدد اول است یا نه
            if n % i == 0:
                # اگر عدد بر عددی تقسیم شود اول نیست
                is_prime = False
                break
            if is_prime:
                # اگه عدد اول بود چاپ میکنیم
                print(n)
